{
    "name": "presttec\/laravel-cashier-paddle",
    "description": "Cashier Paddle provides an expressive, fluent interface to Paddle's subscription billing services.",
    "keywords": [
        "laravel",
        "paddle",
        "billing"
    ],
    "license": "MIT",
    "support": {
        "issues": "https:\/\/github.com\/presttec\/laravel-cashier-paddle\/issues",
        "source": "https:\/\/github.com\/presttec\/laravel-cashier-paddle"
    },
    "authors": [
        {
            "name": "Ramon Oliveira",
            "email": "ramon@presttec.com.br"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "presttec\/guzzle": "^6.5|^7.0",
        "presttec\/illuminate-contracts": "^7.9|^8.0|^9.0",
        "presttec\/illuminate-database": "^7.9|^8.0|^9.0",
        "presttec\/illuminate-http": "^7.9|^8.0|^9.0",
        "presttec\/illuminate-routing": "^7.9|^8.0|^9.0",
        "presttec\/illuminate-support": "^7.9|^8.0|^9.0",
        "presttec\/illuminate-view": "^7.9|^8.0|^9.0",
        "presttec\/money": "^3.2|^4.0",
        "presttec\/carbon": "^2.0",
        "presttec\/url": "^1.3.5|^2.0",
        "presttec\/http-kernel": "^5.0|^6.0",
        "presttec\/polyfill-intl-icu": "^1.22.1"
    },
    "require-dev": {
        "presttec\/mockery": "^1.0",
        "presttec\/testbench": "^5.0|^6.0|^7.0",
        "presttec\/phpunit": "^8.0|^9.0"
    },
    "suggest": {
        "ext-intl": "Allows for more locales besides the default \"en\" when formatting money values."
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Paddle\\": "src\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests\/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Paddle\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replaces": {
        "laravel\/cashier-paddle": "*"
    },
    "conflicts": {
        "laravel\/cashier-paddle": "*"
    }
}